body {
}

.title-text {
  color: #444;
  font-family: 'Helvetica Neue', sans-serif;
  font-size: 35px;
  font-weight: bold;
  line-height: 35px;
  margin-bottom: 10px;
  margin-left: 0px;
  margin-right: 0px;
  margin-top: 0px;
  text-rendering: optimizelegibility;
}

.tooltip.in {
  opacity: 0.8;
  filter: alpha(opacity=100);
}

.sidebar-feed-link {
  margin-top: 15px;
}

.nav-list > li > a {
    background-position: 4px center;
    background-repeat: no-repeat;
    padding-left: 24px;
    height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.nav-list > .active > a,
.nav-list > .active > a:hover {
    background-color: #C5A773;
}

.nav > li > a:hover {
text-decoration: none;
background-color: #FFF3DD;
}

.nav > li > a:hover {
text-decoration: none;
}

.new-group-link > a {
  color:#6FA445;
  font-weight: bold;
}

.page-content {
  max-width: 1170px;
  margin: 0 auto;
}

.posts-title {
  margin-bottom: 10px;
}

.posts-contents {
}

.post-icon {
  transition:  .15s linear;
 -moz-transition:  .15s linear;
 -webkit-transition:  .15s linear;
}

.directory li {
    float: left;
    padding: 0px 10px 5px 0px;
    position: relative;
    list-style: none;
}

.directory li img {
    margin: 0 0;
    padding: 0 0;
    width: 180px;
    height: 180px;
}

.directory li .user-info {
    border-top: 1px solid #DDD;
    padding: 10px;
    background-color: #F3F3F3;
    min-height: 56px;
    height: 56px;
    width: 160px;
    margin-top: 0px;
}

.directory li .user-info .first-name {
    font-size: 18px;
    font-weight: bold;
}

.directory li .user-info .last-name {
    font-size: 12px;
    color: #777;
    display: block;
}

.directory li .user-info .email {
    font-size: 11px;
}

.edit-dongle {
  opacity:0;
  transition:  .15s linear;
 -moz-transition:  .15s linear;
 -webkit-transition:  .15s linear;
}

.edit-dongle a{
 font-weight: 700;
    color: #333;
    opacity: 8;
}

.comment-icon {
  opacity: 0;
  transition:  .15s linear;
 -moz-transition:  .15s linear;
 -webkit-transition:  .15s linear;
}

.post-btn.btn.title-action.actual-post-button {
    background-color: #ADDA88 !important;
    color: #000 !important;
}

.post-btn:hover {
  background-color: #FFF3DD !important;
}

.post-icon:hover {
}

.post-container {
  background: #fff;
  border: 0px;
  padding: 0px;
  font-family: 'Helvetica Neue', sans-serif;
  font-size: 15px;
  font-weight: 300;
  line-height: 30px;
  border-left: 1px dashed lightgrey;
  margin-left: 32px;
  margin-bottom: 4px;
}

.post-container.active{
    background:#eaeaea;
}

.posts-contents > .post-container {
border-left: none;
margin-left: 0px;
}

.post-title {
  color: #e11717;
  font-family: 'Helvetica Neue', sans-serif;
  font-size: 16px;
  font-weight: 500;
  display:block;
  transition:  .10s linear;
 -moz-transition:  .10s linear;
 -webkit-transition:  .10s linear;
}

.post-topic {
  color: #eee;
  font-weight: 500;
  font-size: 13px;

}

.post-body {
  background: #FFF;
  color: #222;
  line-height: 22px;
  margin-left: 30px;
  margin-bottom: 15px;
  padding: 10px;
  border-style: dashed;
  border-width: 1px;
  border-color: #d1d1d1;
  border-radius: 15px;
}

.post-body > h1 {
  font-size: 28px;
}

.post-body > h2 {
  font-size: 24px;
}

.post-body > h3 {
  font-size: 20px;
}

.post-body > h4 {
  font-size: 16px;
}

.post-body.active {
  background:#eaeaea;
}

.post-body:hover{
  background: #FFF3DD;
  border-radius: 15px;
  border-style: solid;
}

.post-voting {
    font-family: 'Helvetica Neue', sans-serif;
    font-size: 18px;
    font-weight: 700;
    color: #dedede;
    float: left;
    line-height: 20px;
    margin-top: 3px;
    margin-left: 8px;
    cursor:pointer;

  }

  .upvote:hover{
    color:#e11717;
  }

  .downvote:hover{
    color:#e11717;
  }

  .upvote.active{
    color:#e11717;
  }

  .downvote.active{
    color:#e11717;
  }

  .upvote {
    margin-bottom: 2px;
     transition:  .10s linear;
    -moz-transition:  .10s linear;
    -webkit-transition:  .10s linear;
  }


  .downvote {
    margin-bottom: 2px;
     transition:  .10s linear;
    -moz-transition:  .10s linear;
    -webkit-transition:  .10s linear;
  }

